In();\n  }\n\n  render() {\n    return (\n      <div className=\"App\">\n        <nav class=\"navbar\">\n          <div class=\"navbar-brand\">\n            <Link class=\"navbar-item\" to=\"/\">\n              <img\n                src=\"https://bulma.io/images/bulma-logo.png\"\n                alt=\"Bulma: a modern CSS framework based on Flexbox\"\n                width=\"112\"\n                height=\"28\"\n              />\n            </Link>\n            <div\n              class=\"navbar-burger burger\"\n              data-target=\"navbarExampleTransparentExample\"\n            >\n              <span></span>\n              <span></span>\n              <span></span>\n            </div>\n          </div>\n\n          <div id=\"navbarExampleTransparentExample\" class=\"navbar-menu\">\n            <div class=\"navbar-start\">\n              <Link class=\"navbar-item\" to=\"/\">\n                Home\n              </Link>\n            </div>\n\n            <label class=\"nav-toggle\" for=\"nav-toggle-state\">\n              <span></span>\n              <span></span>\n              <span></span>\n            </label>\n\n            <input type=\"checkbox\" id=\"nav-toggle-state\" />\n\n            <div class=\"navbar-end\">\n              <div class=\"navbar-item\">\n                <div class=\"field is-grouped\">\n                  <p class=\"control\">\n                    {!this.state.isLogged.username && (\n                      <Link class=\"button is-primary\" to=\"/signup\">\n                        <span class=\"icon\">\n                          <i class=\"fab fa-twitter\"></i>\n                        </span>\n                        <span>Sign Up</span>\n                      </Link>\n                    )}\n                  </p>\n                  <p class=\"control\">\n                    {!this.state.isLogged.username && (\n                      <Link class=\"button is-primary\" to=\"/login\">\n                        <span class=\"icon\">\n                          <i class=\"fab fa-twitter\"></i>\n                        </span>\n                        <span>Log In</span>\n                      </Link>\n                    )}\n                  </p>\n                  <p class=\"control\">\n                    {this.state.isLogged.username && (\n                      <Link class=\"button is-primary\" to=\"/profile\">\n                        <span class=\"icon\">\n                          <i class=\"fab fa-twitter\"></i>\n                        </span>\n                        <span>My Place planner</span>\n                      </Link>\n                    )}\n                  </p>\n                  <p class=\"control\">\n                    {this.state.isLogged.username && (\n                      <button\n                        class=\"button is-primary\"\n                        onClick={() => this.logOut()}\n                      >\n                        <span class=\"icon\">\n                          <i class=\"fab fa-twitter\"></i>\n                        </span>\n                        <span>Log Out</span>\n                      </button>\n                    )}\n                  </p>\n                </div>\n              </div>\n            </div>\n          </div>\n        </nav>\n        \n        {/* <Link to=\"/\">Home Page</Link>\n        <br />\n        <Link to=\"/all-places\">All Places</Link>\n        <br />\n        {!this.state.isLogged.username && <Link to=\"/signup\">Sign Up</Link>}\n        <br />\n        {!this.state.isLogged.username && <Link to=\"/login\">Log In</Link>}\n        <br />\n        {this.state.isLogged.username && <Link to=\"/profile\">Profile</Link>} */}\n\n        <Route\n          exact\n          path=\"/\"\n          render={() => (\n            <Home logOut={this.logOut} isLogged={this.state.isLogged} />\n          )}\n        />\n        <Route exact path=\"/all-places\"\n          render={(props) => {\n            return (\n              <AllPlace